{
  "name": "n8n-nodes-unifi-access",
  "version": "0.1.0",
  "description": "n8n node for Ubiquiti UniFi Access API integration",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "unifi",
    "access",
    "ubiquiti"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ngeorgieff/n8n-unifi-access",
  "author": {
    "name": "ngeorgieff",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ngeorgieff/n8n-unifi-access.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy-files",
    "copy-files": "copyfiles -u 1 \"nodes/**/*.{png,svg,json}\" dist/",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/UnifiAccessApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/UnifiAccess/UnifiAccess.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-n8n-nodes-base": "^1.12.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "n8n-core": "^1.0.0",
    "n8n-workflow": "^1.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
